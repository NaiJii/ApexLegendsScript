}


const asset subbedChampionScreeen = $"ui/champion_screen_nightrun.rpak"
void function ChampionScreenSetExtraInfo( var rui )
{
	RuiSetBool( rui, "isMissionSuccess", FreelanceMode_GetIsVictoryForAll() )
}















































































































































































































































































































